<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    
    <servlet>
        <servlet-name>addcontact</servlet-name>
        <servlet-class>StaffController.AddContact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeContact</servlet-name>
        <servlet-class>CommonController.HomeContact</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>HomeContact</servlet-name>
        <url-pattern>/home-contact</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
        <servlet-name>Booking</servlet-name>
        <servlet-class>UserController.Booking</servlet-class>
    </servlet>
    

   
    
   
   
    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>AminController.CreateAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAccount</servlet-name>
        <servlet-class>AminController.ListAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAccount</servlet-name>
        <servlet-class>AminController.UpdateAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccount</servlet-name>
        <servlet-class>AminController.DeleteAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>AminController.Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStatus</servlet-name>
        <servlet-class>AminController.UpdateStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchAccount</servlet-name>
        <servlet-class>AminController.SearchAccount</servlet-class>
    </servlet>
  
    <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/createaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAccount</servlet-name>
        <url-pattern>/listaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAccount</servlet-name>
        <url-pattern>/updateaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccount</servlet-name>
        <url-pattern>/deleteaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStatus</servlet-name>
        <url-pattern>/updatestatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchAccount</servlet-name>
        <url-pattern>/searchaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    
    
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    
    
  

</web-app>
